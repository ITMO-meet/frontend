{
    "all": true,
    "src": [
        "./src"
    ],
    "include": [
        "src/**"
    ],
    "exclude": [
        "src/index.tsx",
        "src/contexts/**"
    ],
    "extension": [
        ".ts",
        ".tsx"
    ],
    "report-dir": "./coverage",
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80,
    "check-coverage": true
}