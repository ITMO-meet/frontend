{
    "all": true,
    "src": [
        "./src"
    ],
    "include": [
        "src/**"
    ],
    "exclude": [
        "src/index.tsx",
        "src/contexts/**",
        "src/components/basic/IdProps.ts",
        "src/components/examples/**"
        "src/types.ts"
    ],
    "extension": [
        ".ts",
        ".tsx"
    ],
    "report-dir": "./coverage",
    "reporter": ["html", "text"],
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80,
    "check-coverage": true
}