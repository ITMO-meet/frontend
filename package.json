  "description": "",
  "main": "index.jsx",
  "type": "module",
  "scripts": {
    "build": "node build.cjs",
    "start": "serve dist",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test:unit": "mocha -r ts-node/register tests/unit/**/*.spec.ts",
    "test:unit:coverage": "nyc npm rum test:unit --reporter=text",
    "test:e2e": "mocha -r ts-node/register tests/e2e/**/*.spec.ts",
    "test:e2e:coverage": "nyc npm run test:e2e"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 80,
    "functions": 80,
    "branches": 80,
    "statements": 80,
    "all": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "include": [
      "src/**/*.tsx",
      "src/**/*.ts"
    ],
    "exclude": [
      "dist",
      "tests"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
